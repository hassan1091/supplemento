<script>
    function showOnly(){
        var beautyChecked = document.getElementById("beautyCheckbox").checked;
        var sportChecked = document.getElementById("sportCheckbox").checked;
        var vitaminChecked = document.getElementById("vitaminCheckbox").checked;
        showAll()
        if(!(beautyChecked && sportChecked && vitaminChecked) && !(!beautyChecked && !sportChecked && !vitaminChecked)){
            hideAll()
            if(beautyChecked){
                document.querySelectorAll(".beauty").forEach(el => el.style.display = "block")
            }
            if(sportChecked){
                document.querySelectorAll(".sport").forEach(el => el.style.display = "block")
            }
            if(vitaminChecked){
                document.querySelectorAll(".vitamin").forEach(el => el.style.display = "block")
            }
        }
    }
    function showAll(){
        document.querySelectorAll(".beauty, .sport, .vitamin").forEach(el => el.style.display = "block");
    }
    function hideAll(){
        document.querySelectorAll(".beauty, .sport, .vitamin").forEach(el => el.style.display = "none");
    }
</script>
